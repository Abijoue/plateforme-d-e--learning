@page
@model MyApp.Namespace.QstListModel
@{
}


<div class="row mb-2">
    @foreach (var item in Model.questions)
    {
        <div class="col-md-6">
          <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
              <strong class="d-inline-block mb-2 text-primary"></strong>
              <h3 class="mb-0">
                <a class="text-dark" href="#">@item.Qst</a>
              </h3>
              <div class="mb-1 text-muted">@item.ChoixeCorrect</div>
              <p class="card-text mb-auto">
                  <ul>
                    <li>@item.Choix1</li>
                    <li>@item.Choixe2</li>
                    <li>@item.Choix3</li>
                    <li>@item.Choixe4</li>
                </ul>
              </p>
            </div>
          </div>
        </div>
           }
        </div>
      </div>